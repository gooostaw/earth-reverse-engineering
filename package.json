        "start-client": "webpack --config=webpack-client.config.js --mode=development --watch",
        "build": "node tasks.mjs --build",
        "start": "node tasks.mjs --start"
    },
    "pkg": {
        "scripts": "dist/main.js",
        "assets": "dist/public"
    },
